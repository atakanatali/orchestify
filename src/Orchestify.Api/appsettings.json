{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Port=5432;Database=orchestify;Username=orchestify;Password=orchestify_password",
    "Redis": "localhost:6379"
  },
  "RabbitMQ": {
    "Host": "localhost",
    "Username": "orchestify",
    "Password": "orchestify_password",
    "Port": 5672
  },
  "Serilog": {
    "ElasticsearchUrl": "http://localhost:9200",
    "ServiceName": "orchestify-api",
    "Environment": "Development",
    "MinimumLogLevel": "Debug",
    "EnableConsoleSink": true,
    "EnableElasticsearchSink": true
  }
}
